{
    "name": "my-iot-project",
    "version": "1.0.0",
    "description": "An example Truffle project for IoT authentication",
    "main": "truffle-config.js",
    "scripts": {
      "initiate": "npm run venv && npm run activate",
      "start": "npm run dependencies && npm run deploy",
      "test": "truffle test",
      "deploy": "python main.py",
      "venv": "python -m venv myenv",
      "activate": ".\\myenv\\Scripts\\activate",
      "dependencies": "pip install -r requirements.txt",
      "clean": "deactivate && rmdir env /s /q"
    },
    "author": "Venky Tanneru",
    "license": "ISC",
    "dependencies": {
      "@truffle/hdwallet-provider": "^2.1.2",
      "dotenv": "^16.0.0",
      "truffle": "^5.4.14"
    }
  }
  